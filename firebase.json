{
  "database": {
    "rules": "database.rules.json"
  },
  "functions": {
    "predeploy": ["npm --prefix \"$RESOURCE_DIR\" run lint"],
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/account/:name",
        "function": "app"
      }
    ]
  }
}
